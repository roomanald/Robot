#!/usr/bin/env/python

class Robot:
	def Start(self, input):
		print "robot started"
		input.Register
	def Stop(self):
		print "robot stopped"
	def MoveFwd(self):
		print "robot moving forward"
	def MoveBwd(self):
		print "robot moving backward"
	def MoveLeft(self):
		print "robot moving left"
	def MoveRight(self):
		print "robot moving right"
	
